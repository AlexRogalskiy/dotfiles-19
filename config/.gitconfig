[user]
	name = Quinn Rohlf
	email = qr@qrohlf.com
[color]
	ui = true
[push]
	default = tracking
[core]
	editor = subl -n -w
	excludesfile = ~/.dotfiles/config/gitignore_global
[filter "media"]
	clean = git-media-clean %f
	smudge = git-media-smudge %f
[alias]
	c = commit -v
	ac = commit -av
	s = status
	ch = checkout
	oops = commit --amend --no-edit
	unstage = reset HEAD --
[include]
	path = .gitconfig_overrides
[filter "lfs"]
	clean = git-lfs clean %f
	smudge = git-lfs smudge %f
	required = true
